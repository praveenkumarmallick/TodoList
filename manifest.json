  "name": "Chrome Todo Extension",
  "description": "Todo List App Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/todo-logo.png",
    "32": "icons/todo-logo.png",
    "128": "icons/todo-logo.png"
  }
}
